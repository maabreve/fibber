{
  "author": "RedMonkeys",
  "description": "Fibber musicians social network.",
  "name": "fibber",
  "version": "1.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maabreve/fibber.git"
  },
  "scripts": {
    "install_firebug": "wget https://getfirebug.com/releases/lite/latest/firebug-lite.tar.tgz && node decompressFirebug.js",
    "postinstall": "npm run install_firebug && npm run credentials",
    "ng": "ng",
    "start": "run-p build:watch credentials",
    "build:watch": "ng serve",
    "credentials": "node credentials.js && opencollective postinstall",
    "test": "ng test --single-run --code-coverage",
    "test:watch": "ng test --code-coverage",
    "build:dev": "run-p test:watch start",
    "build:dev:firebug": "run-p test:watch build:firebug",
    "build:firebug": "npm start --environment=firebug",
    "build": "ng build",
    "lint": "ng lint --fix",
    "build:client-and-server-bundles": "ng build --prod && ng run browser:server",
    "build:prod": "npm run build:client-and-server-bundles && npm run webpack:server",
    "build:deploy:aws": "npm run build:client-and-server-bundles && npm run webpack:server && npm run deploy:aws",
    "build:deploy:gcloud": "npm run build:client-and-server-bundles && npm run webpack:server && npm run deploy:gcloud",
    "webpack:server": "webpack --config webpack.server.config.js",
    "server": "node local.js",
    "deploy:aws": "cp-cli serverless-aws.yml serverless.yml && serverless deploy && npm run credentials",
    "deploy:gcloud": "cp-cli serverless-gcloud.yml serverless.yml && serverless deploy && npm run credentials"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "schematics",
    "angular",
    "serverless",
    "server side rendering",
    "pwa",
    "progressive web app",
    "lambda",
    "amazon web services",
    "google cloud functions"
  ],
  "dependencies": {
    "@angular-universal-serverless/external-module": "git+https://github.com/maciejtreder/angular-external-module.git",
    "@angular/animations": "^6.0.0-rc.1",
    "@angular/cdk": "^6.0.0-rc.1",
    "@angular/common": "^6.0.0-rc.1",
    "@angular/compiler": "^6.0.0-rc.1",
    "@angular/core": "^6.0.0-rc.1",
    "@angular/forms": "^6.0.0-rc.1",
    "@angular/http": "^6.0.0-rc.1",
    "@angular/material": "^6.0.0-rc.1",
    "@angular/platform-browser": "^6.0.0-rc.1",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.1",
    "@angular/platform-server": "^6.0.0-rc.1",
    "@angular/router": "^6.0.0-rc.1",
    "@angular/service-worker": "^6.0.0-rc.3",
    "@nguniversal/common": "^5.0.0",
    "@nguniversal/express-engine": "^5.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0",
    "@ngx-translate/core": "^10.0.1",
    "@ngx-translate/http-loader": "^3.0.1",
    "aws-serverless-express": "^3.0.0",
    "core-js": "^2.5.4",
    "firebase-admin": "^5.11.0",
    "firebase-functions": "^0.9.1",
    "rxjs": "^6.0.0-rc.0",
    "typescript-collections": "^1.3.2",
    "zone.js": "^0.8.24"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.5.0",
    "@angular/cli": "~6.0.0-rc.2",
    "@angular/compiler-cli": "^6.0.0-rc.1",
    "@angular/language-service": "^6.0.0-rc.1",
    "@types/jasmine": "^2.8.6",
    "@types/node": "^8.0.30",
    "codelyzer": "^4.0.1",
    "cp-cli": "^1.1.0",
    "cpy-cli": "^1.0.1",
    "decompress": "^4.2.0",
    "decompress-targz": "^4.1.1",
    "express": "^4.15.2",
    "http-server": "^0.10.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "node-wget": "^0.4.2",
    "npm-run-all": "^4.1.2",
    "opencollective": "^1.0.3",
    "pre-commit": "^1.2.2",
    "protractor": "~5.1.2",
    "reflect-metadata": "^0.1.10",
    "serverless": "^1.26.1",
    "serverless-apigw-binary": "^0.4.4",
    "serverless-google-cloudfunctions": "^1.1.1",
    "sinon": "^4.5.0",
    "ts-loader": "^4.2.0",
    "tslint": "^5.7.0",
    "typescript": "~2.7.2",
    "webpack-cli": "^2.0.14"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/angular-universal-pwa"
  }
}
